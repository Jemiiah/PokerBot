{
  "name": "@poker/agent",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:blaze": "DOTENV_CONFIG_PATH=../../.env.blaze node -r dotenv/config dist/index.js",
    "start:frost": "DOTENV_CONFIG_PATH=../../.env.frost node -r dotenv/config dist/index.js",
    "start:shadow": "DOTENV_CONFIG_PATH=../../.env.shadow node -r dotenv/config dist/index.js",
    "start:storm": "DOTENV_CONFIG_PATH=../../.env.storm node -r dotenv/config dist/index.js",
    "start:sage": "DOTENV_CONFIG_PATH=../../.env.sage node -r dotenv/config dist/index.js",
    "start:ember": "DOTENV_CONFIG_PATH=../../.env.ember node -r dotenv/config dist/index.js",
    "start:viper": "DOTENV_CONFIG_PATH=../../.env.viper node -r dotenv/config dist/index.js",
    "start:titan": "DOTENV_CONFIG_PATH=../../.env.titan node -r dotenv/config dist/index.js",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@poker/shared": "workspace:*",
    "viem": "^2.21.0",
    "dotenv": "^16.4.0",
    "ws": "^8.16.0",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
